{
  "name": "Tekmar Packet Server",
  "version": "1.0.1",
  "url": "https://github.com/WillCodeForCats/tekmar-packetserv",
  "slug": "tekmar_packetserv",
  "description": "Packet Server Add-On for the Tekmar Gateway 482",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "options": {
    "device": null,
    "ip4_acl": '0.0.0.0/0'
  },
  "schema": {
    "device": "device(subsystem=tty)",
    "ip4_acl": ["match(^\b(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(\/[0-9]{1,2}){0,1}\b$)"]
  },
  "ports": {
    "3000/tcp": null
  },
  "ports_description": {
    "3000/tcp": "Tekmar Packet Server"
  },
  "stage": "stable"
}
